<!DOCTYPE html><html lang="en"><head> <%- include('../../components/head'); %> <title>Studors - Admin</title><style>#user-list{white-space:pre-line}body{height:100%}</style><script charset="utf8"src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script><link rel="stylesheet"type="text/css"href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css"><script charset="utf8"src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script> <%- include('../../components/header'); %> <div class="admin-list"><div id="user-list"><h2>Students:</h2><table id="studentsTable"><thead><tr><th>First Name<th>Last Name<th>Grade<th>Email<th>Phone<tbody> <% for(const obj of students) { %> <tr> <% for(const key in obj) { %> <td><%= obj[key] %></td> <% } %> </tr> <% } %> </table><hr><h2>Tutors:</h2><table id="tutorsTable"><thead><tr><th>First Name<th>Last Name<th>Grade<th>Email<th>Phone<th>Term Hours<th>Total Hours<tbody> <% for(const obj of tutors){ %> <tr> <% for(const key in obj) { %> <td><%= obj[key] %></td> <% } %> </tr> <% } %> </table><hr><h2>Admins:</h2><table id="adminsTable"><thead><tr><th>First Name<th>Last Name<th>Grade<th>Email<th>Phone<tbody> <% for(const obj of admins) { %> <tr> <% for(const key in obj) { %> <td><%= obj[key] %></td> <% } %> </tr> <% } %> </table><hr><h2>Allowed Student domains:</h2><table id="allowedStudentsTable"><thead><tr><th>Domains<tbody> <% for(const obj of studentAllowed) { %> <tr><td><%= obj['email'] %></tr> <% } %> </table><hr><h2>Allowed Tutors:</h2><table id="allowedTutorsTable"><thead><tr><th>Email<tbody> <% for(const obj of tutorAllowed) { %> <tr><td><%= obj['email'] %></tr> <% } %> </table><hr><h2>Allowed Admins:</h2><table id="allowedAdminsTable"><thead><tr><th>Email<tbody> <% for(const obj of adminAllowed) { %> <tr><td><%= obj['email'] %></tr> <% } %> </table><hr><h2>Subjects:</h2><table id="subjectsTable"><thead><tr><th>Subject<tbody> <% for(const obj of subjects) { %> <tr><td><%= obj['subject'] %></tr> <% } %> </table><hr><h2>Time Slots:</h2><table id="incrementsTable"><thead><tr><th>Time<tbody> <% for(const obj of increments) { %> <tr><td><%= functions.formatTime(obj['hour']) %></tr> <% } %> </table><hr><h2>Off Days:</h2><table id="offDaysTable"><thead><tr><th>Date<tbody> <% for(const obj of holidays) { %> <tr><td><%= functions.formatDate(obj['holiday'], false, 0).replace('00:00','') %></tr> <% } %> </table><hr><h2>Upcoming Sessions:</h2><table id="upcomingSessionTable"><thead><tr><th>Time<th>Tutor name<th>Tutor surname<th>Student name<th>Student surname<th>Subject<th>Hours<tbody> <% for(const obj of upcomingSessions) { %> <tr> <% for(const key in obj) { %> <% if(key == "time_id") { %> <td><%= functions.formatDate(obj[key], false, 0) %></td> <% } else { %> <td><%= obj[key] %></td> <% } %> <% } %> </tr> <% } %> </table><hr><h2>Past Sessions:</h2><table id="pastSessionTable"><thead><tr><th>Time<th>Tutor name<th>Tutor surname<th>Student name<th>Student surname<th>Subject<th>Hours<tbody> <% for(const obj of pastSessions) { %> <tr> <% for(const key in obj) { %> <% if(key == "time_id") { %> <td><%= functions.formatDate(obj[key], false, 0) %></td> <% } else { %> <td><%= obj[key] %></td> <% } %> <% } %> </tr> <% } %> </table></div></div> <%- include('../../components/footer'); %> <script>$(document).ready(function(){$("#studentsTable").DataTable(),$("#tutorsTable").DataTable(),$("#adminsTable").DataTable(),$("#allowedStudentsTable").DataTable(),$("#allowedTutorsTable").DataTable(),$("#allowedAdminsTable").DataTable(),$("#subjectsTable").DataTable(),$("#incrementsTable").DataTable(),$("#offDaysTable").DataTable(),$("#upcomingSessionTable").DataTable(),$("#pastSessionTable").DataTable()})</script>